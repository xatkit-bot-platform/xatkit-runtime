<?xml version="1.0" encoding="ASCII"?>
<module:Module xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:module="fr.zelus.jarvis.module" name="SlackModule" jarvisModulePath="fr.zelus.jarvis.plugins.slack.module.SlackModule">
